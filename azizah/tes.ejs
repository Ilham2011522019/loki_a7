  <body>
    <%- include ('modals/headlanding') %>
    <nav>
        <div class="card text-center col-md-10 pt-4 container-fluid">
            <h3>Rencana Pembelajaran Semester</h3><hr>
            <div class="card-body">
              <form class="d-flex" role="search">
                <input class="form-control me-2" type="search" placeholder="Masukkan Nama/Kode Mata Kuliah" aria-label="Search">
                <button class="btn btn-success" type="submit">Cari</button>
              </form>
              <br>
              <table class="table table-striped" id="listrps">
                <thead>
                  <tr>
                    <th scope="col">NO</th>
                    <th scope="col">Kode Mata Kuliah</th>
                    <th scope="col">Nama Mata Kuliah</th>
                    <th scope="col">Jumlah SKS</th>
                    <th scope="col">Semester</th>
                    <th scope="col">Aksi</th>
                  </tr>
                </thead>
                <tbody>
                  <tr>
                    <th scope="row">1</th>
                    <td>JSI62129</td>
                    <td>Pemograman Web</td>
                    <td>3</td>
                    <td>4</td>
                    <td>
                      <a href="" class="btn btn-primary">Lihat</a>
                    </td>
                  </tr>
                  <tr>
                    <th scope="row">2</th>
                    <td>JSI62122</td>
                    <td>Data Mining</td>
                    <td>3</td>
                    <td>4</td>
                    <td>
                      <a href="" class="btn btn-primary">Lihat</a>
                    </td>
                  </tr>
                  <tr>
                    <th scope="row">3</th>
                    <td>JSI62123</td>
                    <td>Perancangan Basis Data</td>
                    <td>4</td>
                    <td>4</td>
                    <td>
                      <a href="" class="btn btn-primary">Lihat</a>
                    </td>
                  </tr>
                </tbody>
              </table>
              </form>
              
              <a href="/login" class="btn btn-success">Log In</a>
            </div>
            <div class="card-footer text-muted">
              Portal Akademik Universitas Andalas
            </div>
        </div>
    </nav>
    <p align="center"><input type="button" onclick="myPrint('listrps')" value ="cetak"></p>
    <script>
      function myPrint(listrps){
        var printdata = document.getElementById(listrps)
        newwin = window.open("");
        newwin.document.write(printdata.outerHTML);
        newwin.print();
        newwin.close();
      }
    </script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0-beta1/dist/js/bootstrap.bundle.min.js" integrity="sha384-pprn3073KE6tl6bjs2QrFaJGz5/SUsLqktiwsUTF55Jfv3qYSDhgCecCxMW52nD2" crossorigin="anonymous"></script>
